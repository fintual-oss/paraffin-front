{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://paraffin-back.herokuapp.com/api/:path*"
    },
    {
      "source": "/users/:path*",
      "destination": "https://paraffin-back.herokuapp.com/users/:path*"
    },
    {
      "source": "/users/auth/google_oauth2/callback",
      "destination": "https://paraffin-back.herokuapp.com/users/auth/google_oauth2/callback"
    },
    {
      "source": "/users/auth/github/callback",
      "destination": "https://paraffin-back.herokuapp.com/users/auth/github/callback"
    },
    {
      "source": "/assets/:path*",
      "destination": "https://paraffin-back.herokuapp.com/assets/:path*"
    }
  ]
}